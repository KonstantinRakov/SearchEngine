{
    "version": 8,
    "configurePresets": [
        {
            "name": "GCC 13.1.0 x86_64-w64-mingw32",
            "displayName": "GCC 13.1.0 x86_64-w64-mingw32",
            "description": "Использование компиляторов: C = C:\\mingw64\\bin\\gcc.exe, CXX = C:\\mingw64\\bin\\g++.exe",
            "generator": "MinGW Makefiles",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
                "CMAKE_C_COMPILER": "C:/mingw64/bin/gcc.exe",
                "CMAKE_CXX_COMPILER": "C:/mingw64/bin/g++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}